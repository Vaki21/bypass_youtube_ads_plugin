{
  "name": "YoutubeBypasser",
  "description": "Base Level Extension",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "index.html",
    "default_icon": "hello_extensions.png"
  },
  "content_scripts": [
    {
      "matches": ["*://*.youtube.com/*"],
      "js": ["popup.js"],
      "run_at": "document_start"
    }
  ],
  "background": {
    "service_worker": "service-worker.js",
    "type": "module"
  },
  "permissions": [
    "http://localhost:1234/*",
    "webNavigation",
    "tabs"
  ]
}
